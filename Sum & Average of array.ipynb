    "import numpy as np\n",
    "x = np.array ([10,20,30,40,50])\n",
    "def average(x):\n",
    "    l = len(x)\n",
    "    add = np.sum(x)\n",
    "    avg = add/l\n",
    "    return avg\n",
    "\n",
    "result = average(x)\n",
    "print (result)"
